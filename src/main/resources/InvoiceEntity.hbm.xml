<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.demo.entities.InvoiceEntity" table="invoice" schema="heroku_8f21e8f014371d8">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(250)" length="250"/>
        </property>
        <property name="created">
            <column name="created" sql-type="date"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(250)" length="250"/>
        </property>
        <property name="accountId">
            <column name="account_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>